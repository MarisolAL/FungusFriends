;; shadow-cljs configuration
{
 :source-paths
 ["src/dev"
  "src/cljs"
  "src/js"]

 :dependencies
 [[re-frame "1.4.0"]
  [reagent "1.1.1"]]

 :dev-http
 {8020 "public"}

 :builds
 {:app
  {:target     :browser
   :output-dir "public/js"
   :asset-path "/js"
   :modules
   {:main ; becomes public/js/main.js
    {:init-fn browser/init}}
   :foreign-libs
   {:js
    {:provides ["fungus-api.ts"]
     :js-module
     {:front-end-api "./src/typescript/front-end-api.js"}}}}}}
